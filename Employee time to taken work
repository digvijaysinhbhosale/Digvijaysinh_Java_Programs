import java.util.Scanner;

public class Employee_Salary 
{
	public static void main(String[] args) 
	{
		double hours;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the time taken by Work");
		hours=sc.nextDouble();
		
		if(hours>=2 && hours<=3)
			System.out.println("highly Efficient");
			
		if(hours>3 && hours<=4)
			System.out.println("Improve Speed");
			
		if(hours>4 && hours<=5)
			System.out.println("Give training to worker");
		
		if(hours>5)
			System.out.println("Worker is Terminated");
	}
}
